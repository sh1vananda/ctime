{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "python -m pip install -r requirements.txt",
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT"
}
